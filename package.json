{
  "name": "aurora-v2",
  "version": "2.0.0",
  "licence": "GPL-3.0-only",
  "author": "Morgan N aka Ichoui",
  "description": "Aurorapp Northern Lights, available on the Play Store only, for Northern Lights Chasers",
  "scripts": {
    "start:front": "ionic serve",
    "start:back": "cd ../aurora-proxy-nest && npm start",
    "android:open": "npx cap open android",
    "android:dev:watch": "ionic cap sync --watch --prod",
    "android:copy-files": "npx cap copy",
    "android:update-files": "npx cap update",
    "android:build:prod": "ionic build --prod",
    "android:create": "npx cap add android",
    "android:trapeze": "npx trapeze run trapeze-config.yaml --android-project android",
    "android:generate-logos-assets": "npx capacitor-assets generate --android --iconBackgroundColor #001e49 --iconBackgroundColorDark #001e49 --splashBackgroundColor #001e49 --splashBackgroundColorDark #001e49 --assetPath src/assets",
    "deploy": "npm run android:build:prod && npm run android:create && npm run android:generate-logos-assets && npm run android:trapeze && npm run android:update-files && npm run android:copy-files",
    "info:ionic": "ionic info",
    "prettier": "prettier --write \"**/*.{js,ts,css,scss,html}\"",
    "prettier:check": "prettier --check \"**/*.{js,ts,css,scss,html}\"",
    "prettier-html": "prettier --write \"./apps/**/src/**/*.html\""
  },
  "private": true,
  "dependencies": {
